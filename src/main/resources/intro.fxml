<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="625.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.but.feec.bds.controllers.ContinueController">
    <stylesheets>
        <URL value="@css/intro.css" />
    </stylesheets>
    <children>
        <!-- Background Image -->
        <ImageView fitHeight="625.0" fitWidth="950.0" preserveRatio="false">
            <image>
                <Image url="@photos/Supreme_bookstore.png" />
            </image>
        </ImageView>

        <!-- Continue Button -->
        <Button fx:id="continueButton" layoutX="405.0" layoutY="550.0" mnemonicParsing="false" onAction="#handleEnterButton" prefHeight="41.0" prefWidth="139.0" text="Continue">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
